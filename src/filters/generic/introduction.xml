    <listitem>
      <para><xref linkend="gimp-filter-normal-map"/></para>
    </listitem>
    <listitem>
      <para><xref linkend="gimp-filter-dilate"/></para>
    </listitem>
    <listitem>
      <para><xref linkend="gimp-filter-erode"/></para>
    </listitem>
    <listitem>
