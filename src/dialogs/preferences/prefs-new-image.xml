      <phrase lang="cs">Předvolby pro nový obrázek</phrase>
      <phrase lang="fr">Préférences de Nouvelle Image</phrase>
      <phrase lang="it">Preferenze nuova immagine</phrase>
      <phrase lang="no">Innstillingane for nytt bilete</phrase>
