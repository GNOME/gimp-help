#: src/using/script-fu-gui-api.xml:91(para)
msgid "Creates a color button in the dialog."
msgstr "Maakt een knop voor een kleur in het dialoogvenster."

#: src/using/script-fu-gui-api.xml:92(para)
msgid "SF-COLOR \"label\" '(red green blue)"
msgstr "SF-COLOR \"label\" '(red green blue)"

#: src/using/script-fu-gui-api.xml:93(para)
msgid "or"
msgstr "of"

#: src/using/script-fu-gui-api.xml:94(para)
msgid "SF-COLOR \"label\" \"color\""
msgstr "SF-COLOR \"label\" \"color\""

#: src/using/script-fu-gui-api.xml:104(seg)
msgid "'(red green blue)"
msgstr "'(red green blue)"

#: src/using/script-fu-gui-api.xml:105(seg)
msgid "List of three values for the red, green and blue components."
msgstr "Lijst met drie waarden voor de rode, groene en blauwe componenten."

#: src/using/script-fu-gui-api.xml:110(seg)
