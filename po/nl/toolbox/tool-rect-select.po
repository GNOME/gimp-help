#: src/toolbox/tool-rect-select.xml:29(secondary)
msgid "Rectangle Selection"
msgstr "Rechthoekselectie Gereedschap"

#: src/toolbox/tool-rect-select.xml:28(primary)
msgid "Tools"
msgstr ""

#: src/toolbox/tool-rect-select.xml:32(primary)
