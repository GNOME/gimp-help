#: src/dialogs/palettes-dialog.xml:14(title)
msgid "Palettes Dialog"
msgstr "Het dialoogvenster Paletten"

#: src/dialogs/palettes-dialog.xml:18(secondary)
#: src/dialogs/palettes-dialog.xml:26(secondary)
msgid "Palettes"
msgstr "Paletten"

#: src/dialogs/palettes-dialog.xml:21(primary)
#: src/dialogs/palettes-dialog.xml:188(primary)
#: src/dialogs/palettes-dialog.xml:206(primary)
#: src/dialogs/palettes-dialog.xml:224(primary)
#: src/dialogs/palettes-dialog.xml:242(primary)
