#: src/filters/enhance/NL_filter.xml:13(title)
msgid "NL Filter"
msgstr ""

#: src/filters/enhance/NL_filter.xml:18(tertiary)
#: src/filters/enhance/NL_filter.xml:21(primary)
msgid "NL filter"
msgstr ""

#: src/filters/enhance/NL_filter.xml:24(primary)
