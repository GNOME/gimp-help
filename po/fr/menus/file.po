#: src/menus/file/save.xml:29(primary)
msgid "Save"
msgstr "Enregistrer"

#: src/menus/file/save.xml:24(primary) src/menus/file/save-as.xml:28(primary)
#: src/menus/file/save-as-template.xml:23(primary)
