#: src/filters/distort/video.xml:56(para)
msgid ""
"You can find this filter through <menuchoice><guimenu>Filters</"
"guimenu><guisubmenu>Distorts</guisubmenu><guimenuitem>Video</guimenuitem></"
"menuchoice>."
msgstr ""
