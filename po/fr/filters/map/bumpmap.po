msgid "Bump Map"
msgstr "Repoussage d'après une carte"

#: src/filters/map/bumpmap.xml:28(primary)
msgid "Filters"
msgstr ""

#: src/filters/map/bumpmap.xml:29(secondary)
msgid "Map"
