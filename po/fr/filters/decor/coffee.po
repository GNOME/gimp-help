#: src/filters/decor/coffee.xml:36(phrase)
msgid "Example for the <quote>Coffee Stain</quote> filter"
msgstr "Exemple d'application pour le filtre <quote>taches de café</quote>"

#: src/filters/decor/coffee.xml:45(para)
