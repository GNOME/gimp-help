#: src/concepts/docks.xml:19(primary)
#: src/concepts/docks.xml:23(secondary)
msgid "Docking"
msgstr "Прикрепление"

#: src/concepts/docks.xml:22(primary)
msgid "Dialogs"
msgstr "Диалоги"

#: src/concepts/docks.xml:27(title)
msgid "Docking Areas"
