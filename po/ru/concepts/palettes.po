#: src/concepts/palettes.xml:29(secondary)
msgid "Introduction"
msgstr "Введение"

#: src/concepts/palettes.xml:32(primary)
msgid "Color"
msgstr "Цвет"

#: src/concepts/palettes.xml:33(secondary)
msgid "Palettes (color map)"
