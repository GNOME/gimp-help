msgid "Delete selected layers"
msgstr "Удалить выбранные слои"

#: src/menus/layer/delete.xml:21(para)
#| msgid ""
#| "The <guimenuitem>Delete Layer</guimenuitem> command deletes the current "
#| "layer from the image."
msgid ""
"The <guimenuitem>Delete Layers</guimenuitem> command deletes the selected "
"layers from the image."
msgstr ""
"Команда <guimenuitem>Удалить слои</guimenuitem> удаляет выбранные слои из "
