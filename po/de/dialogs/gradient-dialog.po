#: src/dialogs/gradient-dialog.xml:22(phrase)
msgid "Gradients Dialog"
msgstr "Der Dialog <quote>Farbverläufe</quote>"

#: src/dialogs/gradient-dialog.xml:25(primary)
msgid "Dialogs"
msgstr ""

#: src/dialogs/gradient-dialog.xml:26(secondary)
