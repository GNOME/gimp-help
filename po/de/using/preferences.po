#: src/using/preferences/prefs-display.xml:48(title)
msgid "Transparency"
msgstr "Transparenz"

#: src/using/preferences/prefs-display.xml:22(secondary)
msgid "Representation"
msgstr "Repräsentation"

#: src/using/preferences/prefs-display.xml:25(primary)
