msgid "0x47"
msgstr ""

#: src/menus/xtns-script-fu.xml:273(phrase)
#: src/menus/xtns-script-fu.xml:334(phrase)
msgid "Magic byte ('G')"
msgstr "Erkennung-(<quote>magisches</quote>)-Byte ('G')"

