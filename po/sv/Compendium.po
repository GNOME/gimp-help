#: src/toolbox/tool-select.xml:29(primary)
#: src/key-reference.xml:978(refentrytitle)
#: src/key-reference.xml:981(refname)
#: src/key-reference.xml:983(guimenuitem)
#: src/key-reference.xml:987(title)
#: src/key-reference.xml:981(refentrytitle)
#: src/key-reference.xml:984(refname)
#: src/key-reference.xml:990(title)
#: src/concepts/concepts.xml:101(term)
