"<menuchoice><guimenu><accel>L</accel>ayer</guimenu><guisubmenu><accel>M</"
"accel>ask</guisubmenu><guimenuitem>Apply Layer <accel>M</accel>ask</"
"guimenuitem></menuchoice>,"
msgstr ""

#: src/menus/layer/mask-apply.xml:55(para)
