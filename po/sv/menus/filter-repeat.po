"<menuchoice><guimenu><accel>F</accel>ilters</guimenu><guimenuitem> "
"Re<accel>p</accel>eat <replaceable>filter</replaceable></guimenuitem></"
"menuchoice>,"
msgstr ""

#: src/menus/filter-repeat.xml:55(para)
msgid ""
