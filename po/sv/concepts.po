msgstr "Ekvation för lagerläge <placeholder-1/>"

#: src/concepts/layer-modes.xml:914(alt)
#, fuzzy
msgid "$$E = 255 - \\frac{256\\times(255 - I)}{M + 1}$$"
msgstr "$E = 255 - \\frac{(255-I)*256}{M+1}$"

