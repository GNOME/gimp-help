#: src/using/script-fu-gui-api.xml:190(para)
msgid "SF-PATTERN \"Pattern\" \"Maple Leaves\""
msgstr "SF-PATTERN \"Pattern\" \"Maple Leaves\""

#: src/using/script-fu-gui-api.xml:191(para)
msgid ""
