#: src/filters/distort/whirlpinch.xml:112(title)
msgid "Parameter Settings"
msgstr "Configuraci√≥ dels par√†metres"

#: src/filters/distort/whirlpinch.xml:114(title)
msgid "<quote>Whirl and Pinch</quote> filter options"
