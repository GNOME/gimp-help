msgid "Semi-Flatten"
msgstr "Semi-aplana"

#: src/filters/web/semiflatten.xml:16(primary)
#: src/filters/web/imagemap.xml:18(primary)
msgid "Filters"
msgstr "Filtres"

#: src/filters/web/semiflatten.xml:17(secondary)
