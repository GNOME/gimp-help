msgstr "<link linkend=\"gimp-tool-scale\">Escalímetre</link>;"

#: src/toolbox/transform/common-features.xml:45(para)
msgid "<link linkend=\"gimp-tool-shear\">Shear</link>;"
msgstr "<link linkend=\"gimp-tool-shear\">Inclina</link>;"

