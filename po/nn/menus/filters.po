msgid "Content"
msgstr "Innhald"

#: src/menus/filters/script-fu.xml:188(entry)
#: src/menus/filters/script-fu.xml:223(entry)
msgid "Description"
msgstr "Beskriving"

#: src/menus/filters/script-fu.xml:193(entry)
#: src/menus/filters/script-fu.xml:228(entry)
