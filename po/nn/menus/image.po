msgstr "<quote>Fjern usynlege lag</quote> er ikkje avkryssa"

#: src/menus/image/merge-layers.xml:144(para)
msgid "<quote>Discard invisible layers</quote> checked"
msgstr "<quote>Fjern usynlege lag</quote> er avkryssa"

