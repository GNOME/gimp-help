#: src/concepts/gradients.xml:14(phrase)
msgid "Gradients"
msgstr "Fargeovergangar"

#: src/concepts/gradients.xml:17(primary)
msgid "Gradient"
msgstr "Fargeovergangar"

#: src/concepts/gradients.xml:18(secondary)
