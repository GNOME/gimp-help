msgstr "Fargeplukkaren"

#: src/toolbox/tool-color-picker.xml:28(primary)
msgid "Tools"
msgstr ""

#: src/toolbox/tool-color-picker.xml:35(primary)
msgid "Eye Dropper"
msgstr ""

#: src/toolbox/tool-color-picker.xml:38(primary)
