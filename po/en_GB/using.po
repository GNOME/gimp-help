"                            theImage theLayer\n"
"                            0 0\n"
"                            inText\n"
"                            0\n"
"                            TRUE\n"
"                            inFontSize PIXELS\n"
"                            inFont)\n"
"                       )\n"
"        )\n"
"      "
msgstr ""
"\n"
"        (set! theText\n"
"                      (car\n"
"                           (gimp-text-fontname\n"
"                            theImage theLayer\n"
"                            0 0\n"
"                            inText\n"
"                            0\n"
"                            TRUE\n"
"                            inFontSize PIXELS\n"
"                            \"Sans\")\n"
"                       )\n"
"        )\n"
