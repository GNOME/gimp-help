msgstr "Webfiltre"

#: src/filters/web.xml:14(primary) src/filters/introduction.xml:19(primary)
#: src/filters/animation.xml:14(primary)
msgid "Filters"
msgstr "Filtre"

#: src/filters/web.xml:15(secondary)
