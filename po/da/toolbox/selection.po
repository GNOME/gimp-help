msgstr "Femtedele"

#: src/toolbox/selection/rectangle.xml:411(para)
msgid "Golden sections"
msgstr "Gyldne snit"

#: src/toolbox/selection/rectangle.xml:414(para)
msgid "Diagonal lines"
msgstr "Diagonale linjer"

#: src/toolbox/selection/rectangle.xml:395(para)
