#: src/dialogs/symmetry-painting.xml:300(para)
msgid "A stroke"
msgstr "Et penselstrøg"

#: src/dialogs/symmetry-painting.xml:249(term)
msgid "Example for Tiling"
msgstr "Eksempel på fliselægning"

#: src/dialogs/symmetry-painting.xml:259(para)
