#: src/concepts/docks.xml:16(phrase)
msgid "Dialogs and Docking"
msgstr "Pannelli e finestre agganciabili"

#: src/concepts/docks.xml:19(primary) src/concepts/docks.xml:23(secondary)
msgid "Docking"
msgstr ""

