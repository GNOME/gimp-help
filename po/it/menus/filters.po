#: src/menus/filters/script-fu.xml:262(entry)
msgid "Content"
msgstr "Contenuto"

#: src/menus/filters/script-fu.xml:228(entry)
#: src/menus/filters/script-fu.xml:263(entry)
msgid "Description"
msgstr "Descrizione"

#: src/menus/filters/script-fu.xml:233(entry)
