#: src/toolbox/color/levels.xml:26(primary)
msgid "Levels tool"
msgstr "La herramienta «Niveles»"

#: src/toolbox/color/levels.xml:29(primary)
msgid "Values"
msgstr "Valores"

#: src/toolbox/color/levels.xml:32(primary)
