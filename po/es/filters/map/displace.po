#: src/filters/map/displace.xml:28(phrase)
#: src/filters/map/displace.xml:33(tertiary)
#: src/filters/map/displace.xml:36(primary)
msgid "Displace"
msgstr "Desplazar"

#: src/filters/map/displace.xml:31(primary)
msgid "Filters"
