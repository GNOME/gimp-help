msgid "Playback"
msgstr "Reproducción"

#: src/filters/animation/playback.xml:24(primary)
msgid "Filters"
msgstr ""

#: src/filters/animation/playback.xml:25(secondary)
#: src/filters/animation/playback.xml:29(primary)
