#: src/menus/layer-align.xml:21(phrase)
msgid "Align Visible Layers"
msgstr "Alinear capas visibles"

#: src/menus/layer-align.xml:24(primary)
msgid "Layer"
msgstr ""

#: src/menus/layer-align.xml:25(secondary)
