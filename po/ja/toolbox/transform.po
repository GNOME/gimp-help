msgid "Adjust"
msgstr "自動調整"

#: src/toolbox/transform/common-features.xml:259(title)
msgid "Original image for examples"
msgstr "元画像とその変化の例"

#: src/toolbox/transform/common-features.xml:266(para)
#: src/toolbox/transform/common-features.xml:369(para)
