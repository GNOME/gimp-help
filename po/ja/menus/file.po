#: src/menus/file/save.xml:18(primary)
msgid "Save"
msgstr "保存"

#: src/menus/file/save.xml:13(primary) src/menus/file/save-as.xml:17(primary)
#: src/menus/file/save-as-template.xml:12(primary)
