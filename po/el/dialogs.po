msgstr "%s"

#: src/dialogs/prefs-image-window-title.xml:85(seg)
msgid "Source scale factor (zoom level = %d/%s)"
msgstr "Συντελεστής κλίμακας πηγής (στάθμη εστίασης = %d/%s)"

#: src/dialogs/prefs-image-window-title.xml:88(seg)
msgid "%d"
msgstr "%d"

