msgid ""
"@@image: 'images/menus/layer/scale.png'; md5=1d82beb03977c6f6a4694b5b8bde8ebe"
msgstr ""
"@@image: 'images/menus/layer/scale.png'; md5=1d82beb03977c6f6a4694b5b8bde8ebe"

