msgid "The Main Toolbox"
msgstr ""

#: src/concepts/toolbox.xml:18(primary)
msgid "Toolbox"
msgstr ""

#: src/concepts/toolbox.xml:19(secondary)
#: src/concepts/script-fu.xml:19(secondary)
